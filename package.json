{
  "name": "project-108",
  "version": "1.0.0",
  "description": "Kiri Vadivelu | Socialist for Scarborough Democracy",
  "main": "index.md",
  "scripts": {
    "generate:sprite": "svg-icon-generate --folder=src/assets/icons --output=src/assets/sprite.svg",
    "watch:eleventy": "eleventy --serve --incremental",
    "watch:sass": "sass  --no-source-map --watch src/sass:src/assets/css",
    "build:eleventy": "eleventy",
    "build:sass": "sass  --no-source-map --style=compressed src/sass:src/assets/css",
    "start": "npm-run-all build:sass generate:sprite --parallel watch:*",
    "build": "npm-run-all build:*",
    "debug": "DEBUG=* npx eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiri-vadivelu/project-108.git"
  },
  "keywords": [
    "\"Toronto\"",
    "\"Socialist\"",
    "\"Election\""
  ],
  "author": "Kiri Vadivelu <kiri@digitaldesigners.ca>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kiri-vadivelu/project-108/issues"
  },
  "homepage": "https://github.com/kiri-vadivelu/project-108#readme",
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11tyrocks/eleventy-plugin-emoji-readtime": "^1.0.1",
    "@quasibit/eleventy-plugin-schema": "^1.10.0",
    "luxon": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.57.1",
    "svg-icon-sprite": "^1.1.1"
  }
}
