<ol class="article__list">
  {% for article in articles %}
  <li class="article__item">
    <p>
      <time>
        <i>{{ article.data.date | readableDate }}</i>
      </time>
      <a href="{{ article.url | url }}">{{ article.data.title }}</a>
      ~{{ article.data.category }}
    </p>
  </li>
  {% endfor %}
</ol>
